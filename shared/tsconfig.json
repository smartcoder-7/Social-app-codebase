{
  "compilerOptions": {
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "esModuleInterop": true,
    "removeComments": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
  }
}